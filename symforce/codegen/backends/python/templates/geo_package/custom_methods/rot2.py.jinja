{%- import "../../util/util.jinja" as util with context -%}

    {# Handwritten methods for Rot2 #}
    {# These will get included into the autogenerated class header. #}
    def __init__(self, z: T.Union[T.Sequence[float], numpy.ndarray, None] = None) -> None:
        if z is None:
            self.data: T.List[float] = ops.GroupOps.identity().data
        else:
            {{ util.flatten_if_ndarray("z", 2) | indent(width=12) }}
            self.data = list(z)

    @classmethod
    def random(cls) -> Rot2:
        return Rot2.random_from_uniform_sample(random.uniform(0, 1))
